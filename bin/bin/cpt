#!/bin/bash

testurl=delay\?timeout\=2000\&url\=http:%2F%2Fwww.gstatic.com%2Fgenerate_204
proxyurl=http://127.0.0.1:9090/proxies

 echo "0:" "$(curl -s $proxyurl/DIRECT/$testurl)"  &
 echo "1:" "$(curl -s $proxyurl/h1/$testurl)"  &
 echo "2:" "$(curl -s $proxyurl/h2/$testurl)"  &
 echo "3:" "$(curl -s $proxyurl/h3/$testurl)"  &
 echo "4:" "$(curl -s $proxyurl/h4/$testurl)"  &
 echo "5:" "$(curl -s $proxyurl/h5/$testurl)"  &
 echo "6:" "$(curl -s $proxyurl/h6/$testurl)"  &
 echo "7:" "$(curl -s $proxyurl/h7/$testurl)"  &
 echo "11:" "$(curl -s $proxyurl/h11/$testurl)"  &
 echo "12:" "$(curl -s $proxyurl/h12/$testurl)"  &
 echo "13:" "$(curl -s $proxyurl/h13/$testurl)"  &
 echo "14:" "$(curl -s $proxyurl/h14/$testurl)"  &
 echo "15:" "$(curl -s $proxyurl/h15/$testurl)"  &
 echo "16:" "$(curl -s $proxyurl/h16/$testurl)"  &
 echo "17:" "$(curl -s $proxyurl/h17/$testurl)"  &
 echo "18:" "$(curl -s $proxyurl/h18/$testurl)"  &
 echo "19:" "$(curl -s $proxyurl/h19/$testurl)"  &
 echo "20:" "$(curl -s $proxyurl/h20/$testurl)"  &
 echo "21:" "$(curl -s $proxyurl/h21/$testurl)"  &
 echo "22:" "$(curl -s $proxyurl/h22/$testurl)"  &
 echo "23:" "$(curl -s $proxyurl/h23/$testurl)"  &

 wait
